{
    "ApiBaseUrl": "https://menominee-api-stage.azurewebsites.net/api",
    "Clients": {
        "AllowedOrigins": "https://menominee-stage.azurewebsites.net/"
    },
    "ConnectionStrings": {
        "Idp": "Azure Secret"
    },
    "DatabaseSettings": {
        "Server": {
            "Name": "Azure Secret",
            "Password": "Azure Secret",
            "UserName": "Azure Secret"
        },
        "Tenant": {
            "Name": "Menominee" // Production tenant name can be found in JWT claims; Development default tenant name is Menominee.
        }
    },
    "IDPSettings": {
        "BaseUrl": "https://menominee-idp-stage.azurewebsites.net/"
    },
    "Logging": {
        "LogLevel": {
            "Microsoft.AspNetCore": "Error",
            "Microsoft": "Warning",
            "Microsoft.EntityFrameworkCore.Database.Command": "Information"
        }
    },
    "OidcConfiguration": {
        "Authority": "https://menominee-idp-stage.azurewebsites.net",
        "PostLogoutRedirectUri": "https://menominee-stage.azurewebsites.net/authentication/logout-callback",
        "RedirectUri": "https://menominee-stage.azurewebsites.net/authentication/login-callback"
    }
}