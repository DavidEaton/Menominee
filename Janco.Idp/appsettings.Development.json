{
  "ConnectionStrings": {
    "UserDbContextConnection": "Server=localhost;Database=JancoIdentity;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "ApplicationSettings": {
    // Access Tokens issues while running in Development need a longer
    // lifetime than only five minutes (the default lifetime).
    // Six months seems reasonable.
    "AccessTokenLifetime": 15700000,
    "MinimumPasswordLength": 8,
    "Environment": "Development",
    "Clients": {
      "RedirectUri": "https://localhost:44307/authentication/login-callback",
      "PostLogoutRedirectUri": "https://localhost:44307/authentication/logout-callback",
      "AllowedCorsOrigin": "https://localhost:44307"
    }
  },
  "Database": {
    "DataSource": "localhost",
    "UserId": "",
    "Password": "",
    "InitialCatalog": "JancoIdentity"
  }
}