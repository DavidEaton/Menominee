@layout TelerikLayout
@inherits LayoutComponentBase

<div>
    <!--header-section declaration -->
    <div class="main-header" id="header-section">
        <ul class="header-list">
            @*<li class="float-left header-style icon-menu" id="hamburger" @onclick="@Toggle"></li>*@
            <li class="float-left header-style nav-pane"><span class="red-text">Stock</span><span class="white-text">Trac</span><span class="sms">Shop Management System</span></li>
            <a href="/">
            <li class="header-style header-style-link float-right">Sign Out</li>
            </a>
            <a href="/">
            <li class="header-style header-style-link float-right">About</li>
            </a>
        </ul>
    </div>
    <!-- end of header-section       -->
    <!-- --------------------------- -->
    <!-- sidebar element declaration -->
    <SfSidebar HtmlAttributes="@HtmlAttribute" Width="180px" DockSize="60px" EnableDock="true" Target=".main-content" @ref="Sidebar" @bind-IsOpen="SidebarExpanded">
        <ChildContent>
            <div class="dock m-sidebar m-menu-scrollbar">
                <ul>
                    <li class="sidebar-item" id="toggle" @onclick="Toggle">
                        <span class="e-icons expand"></span>
                        <span class="e-text" title="menu">Menu</span>
                    </li>
                    <li class="sidebar-item" @onclick="ShowHome">
                        @*<span class="e-icons m-home"></span>*@
                        <span class="material-icons">home</span>
                        <span class="e-text" title="home">Home</span>
                    </li>
                    <li class="sidebar-item" @onclick="ShowDispatch">
                        @*<span class="e-icons m-empty"></span>*@
                        <span class="material-icons">support_agent</span>
                        <span class="e-text" title="dispatch">Dispatch</span>
                    </li>
                    <li class="sidebar-item" @onclick="ShowRepairOrders">
                        <span class="material-icons">car_repair</span>
                        <span class="e-text" title="tickets">Repair Orders</span>
                    </li>
                    <li class="sidebar-item" @onclick="ShowInspections">
                        <span class="material-icons">content_paste_search</span>
                        <span class="e-text" title="inspections">Inspections</span>
                    </li>
                    <li class="sidebar-item" @onclick="ShowSchedule">
                        <span class="material-icons">date_range</span>
                        <span class="e-text" title="schedule">Schedule</span>
                    </li>
                    <li class="sidebar-item" @onclick="ShowPartOrders">
                        <span class="material-icons">shopping_cart</span>
                        <span class="e-text" title="orders">Parts Orders</span>
                    </li>
                    <li class="sidebar-item" @onclick="ShowInventory">
                        <span class="material-icons">warehouse</span>
                        <span class="e-text" title="inventory">Inventory</span>
                    </li>
                    <li class="sidebar-item" @onclick="ShowReports">
                        <span class="material-icons">bar_chart</span>
                        <span class="e-text" title="reports">Reports</span>
                    </li>
                    <li class="sidebar-item" @onclick="ShowCustomers">
                        <span class="material-icons">people</span>
                        <span class="e-text" title="customers">Customers</span>
                    </li>
                    <li class="sidebar-item" @onclick="ShowReceivables">
                        <span class="material-icons">savings</span>
                        <span class="e-text" title="receivables">Receivables</span>
                    </li>
                    <li class="sidebar-item" @onclick="ShowPayables">
                        <span class="material-icons">payments</span>
                        <span class="e-text" title="payables">Payables</span>
                    </li>
                    <li class="sidebar-item" @onclick="ShowEmployees">
                        <span class="material-icons">assignment_ind</span>
                        <span class="e-text" title="employees">Employees</span>
                    </li>
                    <li class="sidebar-item" @onclick="ShowSettings">
                        <span class="material-icons">settings</span>
                        <span class="e-text" title="settings">Settings</span>
                    </li>
                </ul>
            </div>
        </ChildContent>
    </SfSidebar>
    <!-- end of sidebar element   -->
    <!-- ------------------------ -->
    <!-- main-content declaration -->
    <div class="main-content full-height" id="main-text">
        <div class="sidebar-content">
            <div id="main-body">
                @Body
            </div>
        </div>
    </div>
    <!-- end of main-content declaration -->
</div>

