{
  "ApiBaseUrl": "https://menominee-api.azurewebsites.net/api/",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "OidcConfiguration": {
    "Authority": "https://menominee-idp.azurewebsites.net",
    "ClientId": "menominee-client",
    "DefaultScopes": [
      "email",
      "menominee-api",
      "tenantId",
      "tenantName",
      "role",
      "shopRole",
      "subscriptionLevel",
      "subscribedProducts"
    ],
    "RedirectUri": "https://menominee.azurewebsites.net/authentication/login-callback",
    "PostLogoutRedirectUri": "https://menominee.azurewebsites.net/authentication/logout-callback",
    "ResponseType": "code"
  },
  "UserOptions": {
    "NameClaim": "email"
  },
  "TenantLogoImageErrorUrl": "https://menominee.blob.core.windows.net/new-tenant/new-tenant",
  "Tenant": {
    "LogoImage": {
      "BaseUrl": "https://menominee.blob.core.windows.net/",
      "ErrorImage": "new-tenant/new-tenant"
    }
  }
}